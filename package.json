{
  "name": "crucible-standard",
  "version": "0.0.1",
  "description": "A news site for news",
  "license": "ISC",
  "contributors": [
    "Beau Bouchard <https://github.com/BeauBouchard>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/free-ignorance/crucible-standard"
  },
  "bugs": {
    "url": "https://github.com/free-ignorance/crucible-standard/issues"
  },
  "homepage": "https://github.com/free-ignorance/crucible-standard#readme",
  "eslintConfig": {
    "extends": "airbnb"
  },
  "dependencies": {
    "gatsby": "2.8.5",
    "gatsby-image": "2.1.2",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-offline": "2.1.1",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.1.3",
    "gatsby-source-filesystem": "2.0.38",
    "gatsby-transformer-sharp": "2.1.21",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "nyc": "14.1.1",
    "prettier": "1.18.1"
  },
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "cover": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "node_modules/.bin/eslint --format codeframe './lib/**/*.js'",
    "lint-fix": "node_modules/.bin/eslint --fix --format codeframe './lib/**/*.js'"
  }
}
